# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

## HashMap Technique

class Solution(object):
    def hasCycle(self, head):
        a=set()
        while head:
            if head in a:
                return True
            a.add(head)
            head=head.next
        return False
            
